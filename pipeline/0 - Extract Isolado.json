				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SET NOCOUNT ON\n\nSelect\n\tROW_NUMBER() OVER(ORDER BY (GETDATE())) as ID\n\t,A.CICLO + A.CRM AS CRM_CICLO\n\t,RIGHT(A.CICLO,2) AS MES\t  \n\t,A.CICLO\n    ,A.CRM\n    ,A.MEDICO\n    ,A.TIPO\n    ,A.TIPOCONTATO\n    ,A.GRUPOPROMOCIONAL\n    ,A.ESPECIALIDADE\n    ,A.LOGRADOURO\n    ,A.COMPLEMENTO\n    ,A.BAIRRO\n    ,A.CIDADE\n    ,A.UF\n    ,A.CEP\n    ,A.BRICK\n    ,A.TELEFONEMEDICO\n    ,A.CELULARMEDICO\n    ,A.EMAIL AS EMAIL_1_1\n    ,A.ROW_ID_FUNCIONARIO\n    ,A.ROW_ID_TERRITORIO\n    ,A.ROW_ID_CONTATO\n    ,A.STATUS\n    ,A.QTD_VISITA_PLANEJADA AS QTD_VISITA_PLANEJADA_PAINEL\n\t,A.QTD_VISITA_REALIZADA AS QTD_VISITA_REALIZADA_PAINEL\n    ,A.ESPECIALIDADE_02\n    ,A.ESPECIALIDADE_03\n    ,A.VP_PROX\n    ,A.STATUS_MEDICO\n    ,A.EMAIL_CONSENT AS EMAIL_1_1_CONSENT\n    ,B.NETWORK_ID\n    ,B.RX_GIGYA_ID\n    ,D.VEEVA_ID\n    ,B.ROW_ID_CONTATO_EXT\n    ,A.OUTRO_EMAIL AS EMAIL_MASS\n    ,A.OUTRO_EMAIL_CONSENT  AS EMAIL_MASS_CONSENT  \n    ,CASE \n\t\tWHEN C.CYCLE_PLAN = 'S' THEN 'S'\n\t ELSE 'N' END \t\t\t\t\t\t\t\t\t\t\t\t\t\t\tAS CYCLE_PLAN,\n\t GP.MERCADO,\n\t GP.GRUPOPROMOCIONAL as 'PRODUTO_PAINEL'\nFROM\n\tDIM_PAINEL_CORRENTE A LEFT OUTER JOIN STG_CAD_CONTAS_VEEVA B\n    ON\tA.ROW_ID_CONTATO = B.ROW_ID_CONTATO\n\n\tLeft join Dim_Cad_Grade_Promocional_Aberta_Qlikview as GP\n\ton A.CRM = GP.CRM AND A.ROW_ID_TERRITORIO = GP.ROW_ID_TERRITORIO\n    \n    LEFT JOIN Dim_Sense_Universo_Medico D\n      ON D.CRM = B.DOCUMENTO\n\t  AND D.NETWORK_ID = B.NETWORK_ID\n    \n    LEFT OUTER JOIN View_Sense_Cycle_Plan C\n\tON\tA.CICLO = C.CICLO AND\n\t\tA.ROW_ID_CONTATO = C.ROW_ID_CONTATO AND\n\t\tA.ROW_ID_TERRITORIO = C.ROW_ID_TERRITORIO\n    \nWHERE\n\tLEFT(CAST(A.CICLO AS VARCHAR(10)),4) IN (YEAR(GETDATE()),YEAR(GETDATE())-1) AND\n    EXISTS (SELECT B.* FROM DIM_CAD_FORCA_VENDAS B WHERE A.CICLO = B.CICLO AND A.ROW_ID_TERRITORIO = B.ROW_ID_TERRITORIO \n            AND B.FLAG_BI IS NULL)\nOPTION(FAST 10000)",
						"queryTimeout": "08:20:00",
						"partitionOption": "None"
					},
