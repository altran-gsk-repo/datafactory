					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT    \n\tA.ROW_ID_CONTATO,\t\n    A.ROW_ID_TERRITORIO,\n    A.CICLO,\t\n    A.PRODUTO AS PRODUTO_FRANCHISE,\t\n    A.FRANCHISE,\t\t\n    A.SUB_CATEGORY AS SUB_CAT_FRANCHISE,\n    E.PESO AS PESO_FRANCHISE,\n\t'VF'\t\t\t\t\t\t\t\t\t\tas TIPO_INTERACAO\nFROM\n\t\"GSK_Intelligence\".dbo.\"Fato_Visitas_Realizadas_Franchise\" A \n    LEFT OUTER JOIN (\tSELECT \n                          A.CICLO,\n                          A.ROW_ID_TERRITORIO,\n                          B.PESO,\n                          CASE\n                          WHEN RIGHT(TERRITORIO,6) = '022010' THEN 'MSL LUPUS'\n                          WHEN RIGHT(TERRITORIO,6) = '037000' THEN 'MSL HEALTHECO'\n                          WHEN RIGHT(TERRITORIO,6) = '022001' THEN 'MSL VIIV'\n                          WHEN RIGHT(TERRITORIO,6) = '022011' THEN 'MSL VIIV'\n                          WHEN RIGHT(TERRITORIO,6) = '022012' THEN 'MSL VIIV'\n                          WHEN RIGHT(TERRITORIO,6) = '022007' THEN 'MSL RESP'\n                          WHEN RIGHT(TERRITORIO,6) = '022009' THEN 'MSL RESP'\n                          WHEN RIGHT(TERRITORIO,6) = '036000' THEN 'MSL DPOC'\n                          WHEN RIGHT(TERRITORIO,6) = '055001' THEN 'MSL VACINAS'\n                          WHEN RIGHT(TERRITORIO,6) = '055002' THEN 'MSL VACINAS'\n                          WHEN RIGHT(TERRITORIO,6) = '055003' THEN 'MSL VACINAS'\n                          WHEN RIGHT(TERRITORIO,6) = '055004' THEN 'MSL VACINAS'\n                          WHEN RIGHT(TERRITORIO,6) = '055005' THEN 'MSL VACINAS'\n                          WHEN RIGHT(TERRITORIO,6) = '055006' THEN 'MSL VACINAS'\n                          ELSE A.DIVISÃO END AS DIVISAO,\n                          B.PRODUTO\n                          FROM\n\t\t\t\t\t\t\t  \"GSK_Intelligence\".dbo.\"DIM_CAD_FORCA_VENDAS\" A LEFT OUTER JOIN (SELECT * FROM \"GSK_Intelligence\".dbo.\"Dim_Cad_FTE_Peso_Produto\" WHERE PESO <> '0') B\n\t\t\t\t\t\t\t  ON  CASE\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022010' THEN 'MSL LUPUS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '037000' THEN 'MSL HEALTHECO'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022001' THEN 'MSL VIIV'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022011' THEN 'MSL VIIV'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022012' THEN 'MSL VIIV'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022007' THEN 'MSL RESP'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '022009' THEN 'MSL RESP'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '036000' THEN 'MSL DPOC'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055001' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055002' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055003' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055004' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055005' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  WHEN RIGHT(TERRITORIO,6) = '055006' THEN 'MSL VACINAS'\n\t\t\t\t\t\t\t\t\t  ELSE A.DIVISÃO END = B.DIVISAO\n\n                          WHERE\n                          LEFT(CAST(A.CICLO AS VARCHAR(10)),4) IN (YEAR(GETDATE()),YEAR(GETDATE())-1) AND\n                          EXISTS (SELECT C.* FROM DIM_CAD_FORCA_VENDAS C WHERE A.CICLO  = C.CICLO AND A.ROW_ID_TERRITORIO = C.ROW_ID_TERRITORIO \n                           \n                          AND C.FLAG_BI IS NULL)) E\n    ON\tA.CICLO = E.CICLO AND\n\t\tA.ROW_ID_TERRITORIO = E.ROW_ID_TERRITORIO AND\n\t\tA.PRODUTO = E.PRODUTO;",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
